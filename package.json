{
  "name": "prototipo-gestor-cash",
  "version": "1.0.0",
  "private": true,
  "description": "Sistema de gesti√≥n de caja - Backend Node.js/Express + Frontend Next.js",
  "scripts": {
    "use:local": "npx cross-env ACTIVE_DB=local USE_SSL=false docker compose up --build",
    "use:supabase": "npx cross-env ACTIVE_DB=supabase USE_SSL=true docker compose up --build",
    "dev": "cd app/web && npm run dev",
    "dev:web": "cd app/web && npm run dev",
    "dev:server": "cd app/server && npm run dev",
    "dev:all": "concurrently \"npm run dev:server\" \"npm run dev:web\"",
    "build": "cd app/web && npm run build",
    "build:server": "cd app/server && npm run build",
    "start": "cd app/web && npm start",
    "start:server": "cd app/server && npm start",
    "lint": "cd app/web && npm run lint",
    "lint:server": "cd app/server && npm run lint",
    "typecheck": "cd app/web && npm run typecheck",
    "typecheck:server": "cd app/server && npm run typecheck",
    "install:web": "cd app/web && npm install",
    "install:server": "cd app/server && npm install",
    "install:all": "npm run install:server && npm run install:web"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "gestor-cash",
    "cash-management",
    "nextjs",
    "express",
    "nodejs"
  ],
  "author": "",
  "license": "ISC",
  "workspaces": [
    "app/web",
    "app/server"
  ],
  "dependencies": {
    "postgres": "^3.4.7"
  }
}
